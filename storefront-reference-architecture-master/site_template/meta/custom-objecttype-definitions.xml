<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">
    <custom-type type-id="ACCOUNT_VERIFICATION_DATA">
        <display-name xml:lang="x-default">Account verification data</display-name>
        <staging-mode>no-staging</staging-mode>
        <storage-scope>site</storage-scope>
        <retention-days>1</retention-days>
        <key-definition attribute-id="id">
            <type>string</type>
            <min-length>0</min-length>
        </key-definition>
        <attribute-definitions>
            <attribute-definition attribute-id="email">
                <display-name xml:lang="x-default">Email</display-name>
                <type>email</type>
                <localizable-flag>false</localizable-flag>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
            <attribute-definition attribute-id="password">
                <display-name xml:lang="x-default">Password</display-name>
                <type>password</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
            </attribute-definition>
        </attribute-definitions>
        <group-definitions>
            <attribute-group group-id="Standart">
                <display-name xml:lang="x-default">Account verification data</display-name>
                <attribute attribute-id="id"/>
                <attribute attribute-id="email"/>
                <attribute attribute-id="password"/>
            </attribute-group>
        </group-definitions>
    </custom-type>

</metadata>
